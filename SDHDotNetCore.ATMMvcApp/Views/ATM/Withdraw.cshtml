@model ATMUserModel
@{
    ViewData["Title"] = "Withdraw";
}

<h2>Withdraw</h2>
<form method="post" action="/ATM/Withdraw/@Model.UserId">
    <label for="amount">Amount:</label>
    <input type="number" id="amount" name="amount" required>
    <br>
    <button type="submit">Withdraw</button>
</form>
